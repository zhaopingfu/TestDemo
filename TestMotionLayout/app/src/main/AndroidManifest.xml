<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.meta.testmotionlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fly_in_helper.FlyInActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CircleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FlowActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ConstraintSetActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".PlaceHolderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BarrierActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".circular_reveal_helper.CircularRevealActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".LayerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".GroupActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WithCoordinatorLayoutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".WithDrawerLayoutActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ObjectAnimatorActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>